{
  "name": "playground",
  "version": "1.0.0",
  "description": "React and friends",
  "repository": "git@github.com:sunstorymvp/playground.git",
  "author": "sunstorymvp",
  "license": "MIT",
  "scripts": {
    "start": "cross-env NODE_ENV=development webpack-dev-server --progress --env.development",
    "build": "cross-env NODE_ENV=production webpack --env.production",
    "analyze:bundle": "cross-env NODE_ENV=production webpack --env.analyze",
    "analyze:copypaste": "jscpd --limit 5",
    "lint": "concurrently 'npm run lint:eslint -s' 'npm run lint:stylelint -s'",
    "lint:eslint": "eslint --cache --max-warnings=0 .",
    "lint:stylelint": "stylelint **/src/**/*.css --cache",
    "test": "cross-env NODE_ENV=test jest",
    "test:watch": "npm run test -s -- --watch",
    "test:coverage": "npm run test -s -- --coverage"
  },
  "dependencies": {
    "classnames": "~2.2.5",
    "lodash": "~4.17.4",
    "moment": "~2.18.1",
    "normalize.css": "~7.0.0",
    "prop-types": "~15.5.8",
    "react": "~15.5.4",
    "react-dom": "~15.5.4",
    "react-helmet": "~5.0.2",
    "react-redux": "~5.0.1",
    "react-router": "~3.0.1",
    "react-router-redux": "~4.0.7",
    "redux": "~3.6.0",
    "redux-actions": "~2.0.1",
    "redux-logger": "~3.0.0",
    "redux-thunk": "~2.2.0",
    "uuid": "~3.1.0"
  },
  "devDependencies": {
    "babel-core": "~6.24.0",
    "babel-eslint": "~8.0.1",
    "babel-jest": "~20.0.0",
    "babel-loader": "~7.0.0",
