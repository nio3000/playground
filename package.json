    "webpack-notifier": "~1.5.0"
  },
  "scripts": {
    "start:development": "cross-env NODE_ENV=development webpack-dev-server --hot --env.development",
    "compile:development": "cross-env NODE_ENV=development webpack --env.development",
    "compile:production": "cross-env NODE_ENV=production webpack --env.production",
    "build": "npm run compile:production -s && npm run minify -s",
    "analyze:bundle": "cross-env NODE_ENV=production webpack --env.analyze",
    "analyze:copypaste": "jscpd --limit 5",
    "minify": "babili dist -d dist --no-comments",
    "lint": "npm run lint:eslint -s & npm run lint:stylelint -s",
    "lint:eslint": "eslint --cache .",
    "lint:stylelint": "stylelint **/src/**/*.css",
    "test": "cross-env NODE_ENV=test jest --config .jestrc",
    "test:watch": "npm run test -s -- --watch",
    "test:coverage": "npm run test -s -- --coverage"
  },
  "engines": {
    "node": ">=6.9.1"
  }
}
